<div class="page-header">
  <h3>Food Photo #<%= @food_photo.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @food_photo.user_id %></dd>

      <dt>Menu item</dt>
      <dd><%= @food_photo.menu_item_id %></dd>

      <dt>Item review</dt>
      <dd><%= @food_photo.item_review %></dd>

      <dt>Comment</dt>
      <dd><%= @food_photo.comment %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/food_photos" class="btn btn-primary">
        Back
      </a>
      <a href="/food_photos/<%= @food_photo.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_food_photo/<%= @food_photo.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
