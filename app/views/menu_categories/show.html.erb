<div class="page-header">
  <h3>Menu Category #<%= @menu_category.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Menuid</dt>
      <dd><%= @menu_category.MenuId %></dd>

      <dt>Categoryid</dt>
      <dd><%= @menu_category.CategoryId %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/menu_categories" class="btn btn-primary">
        Back
      </a>
      <a href="/menu_categories/<%= @menu_category.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_menu_category/<%= @menu_category.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
